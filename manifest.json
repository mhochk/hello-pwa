  "theme_color": "#8E2C54",
  "scope":".",
  "background_color": "#2C488E",
  "display": "minimal-ui",
  "orientation": "portrait",
  "icons": [
    {
        "src": "/hello-pwa/skull192.png",
        "sizes": "192x192",
        "type": "image/png"
    }, {
      "src": "/hello-pwa/skull512.png",
        "sizes": "512X512",
        "type": "image/png"
    }
  ],
  "share_target": {
    "action": "/share-target/",
    "method": "GET",
    "enctype": "multipart/form-data",
    "params": {
      "title": "mapped_title",
      "text": "mapped_text",
      "url": "mapped_url",
      "files": [
        {
          "name": "mapped_files",
          "accept": ["*"]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/simple-text-reader",
      "accept": {
        "text/csv": ".txt5"
      }
    },
    {
      "action": "/",
      "accept": {
        "text/csv": [ ".txt2, txt3" ],
        "application/vnd.alternative-graph-app.graph": ".graph2"
      }
    }
  ],
  "related_applications": [
    {
      "platform": "browser",
      "id": "Microsoft.MicrosoftEdge.Stable_8wekyb3d8bbwe"
    },
    {
      "platform": "browser",
      "id": "Microsoft.MicrosoftEdge.Canary_8wekyb3d8bbwe"
    },
    {
      "platform": "play",
      "id": "android-mhochk-app",
      "url": "https://mhochk.github.io/hello-pwa"
    }
  ]
